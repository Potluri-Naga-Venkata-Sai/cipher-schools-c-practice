// Ques 1: You are given an array and target value. Your task is to find the target value that exists in the array. If it exists inside an array then print YES otherwise NO.

// Sample Input :- Array -[2,4,6,7,8]
// Target: 6
// Output: YES

#include<bits/stdc++.h>
using namespace std;
int main(){
    int n;
    cin>>n;
    int arr[n];
    for(int i=0;i<n;i++){
        cin>>arr[i];
    }
    int ele;
    cin>>ele;
    int flag=0;
    for(int i=0;i<n;i++){
        if(arr[i]==ele){
            flag=1;
        }
    }
    if(flag!=0){
        cout<<"yes";
    }else{
        cout<<"no";
    }
}

// Ques 2: You are given an array and target value. You have to find the index of target value. If target doest exist inside the array then print -1.Assume indexing is 1 based.

// Sample Input :- Array -[2,4,6,7,8]
// Target: 6
// Output: 3

#include<bits/stdc++.h>
using namespace std;
int index(int arr[],int n,int ele){
        for(int i=0;i<n;i++){
        if(arr[i]==ele){
            return i+1;
        }
    }
    return -1;
}
int main(){
    int n;
    cin>>n;
    int arr[n];
    for(int i=0;i<n;i++){
        cin>>arr[i];
    }
    int ele;
    cin>>ele;
    int result=index(arr,n,ele);
    cout<<result;

}
